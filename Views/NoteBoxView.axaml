<UserControl xmlns="https://github.com/avaloniaui"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
             mc:Ignorable="d" d:DesignWidth="800" d:DesignHeight="450"
             x:Class="NoteTalking.Views.NoteBoxView">
  <DockPanel x:Name="NotesPanel" Width="500" Background="ForestGreen" Grid.Row="1">
    <StackPanel Margin="10" HorizontalAlignment="Left" VerticalAlignment="Top" Width="50" Height="50" Background="Fuchsia" DockPanel.Dock="Left"></StackPanel>
    <StackPanel Margin="10" HorizontalAlignment="Left" VerticalAlignment="Top" Width="50" Height="50" Background="Fuchsia" DockPanel.Dock="Left"></StackPanel>
    <Button Command="{Binding $parent[Window]}" Content="Beep" Margin="50" HorizontalAlignment="Right" VerticalAlignment="Bottom" DockPanel.Dock="Bottom"> </Button>
  </DockPanel>
</UserControl>
